                    android:layout_centerVertical="true"
                    android:scaleType="fitXY"
                    android:src="@drawable/tb_shezhi" />
            </LinearLayout>

            <TextView
                android:id="@+id/shezhi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
